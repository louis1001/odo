
class Vector{
    double x
    double y
    init(double x_ = 0.0, double y_ = 0.0){
        x = x_
        y = y_
    }

    func hypot() : double{
        return sqrt(pow(x, 2.0) + pow(y, 2.0))
    }

    func copy() : Vector{
        return new Vector(x, y)
    }
}

class Printer{
    Vector ref
    init(Vector v){
        ref = v
    }

    func printX(){
        println(ref.x)
    }

    func info() {
        println("X: \t", ref.x)
        println("Y: \t", ref.y)
        println("Len: \t", ref.hypot())
    }
}

func printX(Vector v){
    println(v.x)
}

Vector a = new Vector(1.0, 2.0)
print("a.x: ")
printX(a)

Vector some = a.copy()
print("some.x: ")
println(some.x)


Printer pri = new Printer(a)
print("pri.x: ")
pri.printX()

println()
println("a.x = 4")
a.x = 4.0
println()

print("a.x: ")
printX(a)

print("some.x: ")
println(some.x)

print("pri.x: ")
pri.printX()
println()

println("A:")
pri.info()
