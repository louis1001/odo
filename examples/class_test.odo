
class Vector{
    var x: double
    var y: double
    init(x_ = 0.0, y_ = 0.0){
        x = x_
        y = y_
    }

    func hypot() : double{
        return sqrt(pow(x, 2) + pow(y, 2))
    }

    func copy() : Vector{
        return new Vector(x, y)
    }
}

class Printer{
    var ref: Vector
    init(v: Vector){
        ref = v
    }

    func printX(){
        writeln(ref.x)
    }

    func info() {
        writeln("X: \t", ref.x)
        writeln("Y: \t", ref.y)
        writeln("Len: \t", ref.hypot())
    }
}

func printX(v: Vector){
    writeln(v.x)
}

var a = new Vector(1, 2)
write("a.x: ")
printX(a)

var some = a.copy()
write("some.x: ")
writeln(some.x)


var pri = new Printer(a)
write("pri.x: ")
pri.printX()

writeln()
writeln("a.x = 4")
a.x = 4
writeln()

write("a.x: ")
printX(a)

write("some.x: ")
writeln(some.x)

write("pri.x: ")
pri.printX()
writeln()

writeln("A:")
pri.info()
