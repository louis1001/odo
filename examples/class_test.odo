
class Vector{
    double x
    double y
    init(double x_ = 0, double y_ = 0){
        x = x_
        y = y_
    }

    func hypot() : double{
        return sqrt(pow(x, 2) + pow(y, 2))
    }

    func copy() : Vector{
        return new Vector(x, y)
    }
}

class Printer{
    Vector ref
    init(Vector v){
        ref = v
    }

    func printX(){
        writeln(ref.x)
    }

    func info() {
        writeln("X: \t", ref.x)
        writeln("Y: \t", ref.y)
        writeln("Len: \t", ref.hypot())
    }
}

func printX(Vector v){
    writeln(v.x)
}

Vector a(1, 2)
write("a.x: ")
printX(a)

Vector some = a.copy()
write("some.x: ")
writeln(some.x)


Printer pri(a)
write("pri.x: ")
pri.printX()

writeln()
writeln("a.x = 4")
a.x = 4
writeln()

write("a.x: ")
printX(a)

write("some.x: ")
writeln(some.x)

write("pri.x: ")
pri.printX()
writeln()

writeln("A:")
pri.info()
