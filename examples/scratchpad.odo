class A {
    static func static_function() {
        writeln("Calling a static function")
        # This is it. The whole point of lazy checking.
        A temp()
        writeln(temp)
    }

    init() {
        writeln("Hey! You constructed an A")
    }
}

func main() {
    writeln("Starting the program")
    test_class()
}

func test_class() {
    writeln("Gonna test the class' lazy initializer.")

    A a()
    a::static_function()
}

main()
