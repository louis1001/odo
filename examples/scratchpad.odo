#int a = maprange x: 20 { x*10 }

int count = 5
double numbers[] = []

forange i ~: count {
    numbers += i
}

println("This is the result: ", numbers, "\n")

println("Forwards")
foreach n : numbers {
    println("This made it even betterer! ", n)
}

println("\nAnd then backwards")
foreach n ~: numbers {
    println("This made it even betterer! ", n)
}
