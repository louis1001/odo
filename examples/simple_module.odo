module math{
    double e = 2.71828183198665
    func sq(int x): double {
        return x*x
    }

    func exp(double x): double {
        return e^x
    }

    module nested {
        string works = "YES!!"
    }
}

double a = 3.5

double result = math::exp(a)
println("E to the ", a, " is ", result)

println("\nAccessing a value in a nested module:")
println(math::nested::works)
