any greetings_func = func (string name, int messages, double temp) {
    writeln(
        "Hello, ", name, ". You have ", messages,
        " new messages. The temparature is ", temp,
        " degrees."
    )
}

double i = read_double("Write a double: ")

int j = read_int("Write a int: ")

string name = read("Write your name: ")

clear()
greetings_func(name, j, i)

wait()
